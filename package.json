{
  "name": "nestjs-keycloak",
  "version": "1.5.1",
  "description": "crud for restful apis built with nestjs and prisma",
  "keywords": [
    "nestjs-keycloak",
    "nestjs",
    "prisma",
    "crud",
    "api",
    "rest"
  ],
  "homepage": "https://github.com/codejamninja/nestjs-keycloak",
  "bugs": {
